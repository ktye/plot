#include<stdio.h>
#include<stdlib.h>
#include<stddef.h>
#include<string.h>
#include<math.h>
typedef void V;
typedef double F;
typedef int I;
V help() {   // echo "disable r" >> ~/.zshrc
	printf("x axis generator\n");
	printf("x [N]  $xmin $xmax\n");
	exit(1);
}
I main(int args, char **argv) {
	I n    = atoi(getenv("N"));
	F xmin = atof(getenv("xmin"));
	F xmax = atof(getenv("xmax"));
	F ymin = atof(getenv("xmin"));
	F ymax = atof(getenv("ymax"));
	if(args>1) {
		if(argv[1][0] == '-') help();
		n=atoi(argv[1]);
	}
	if(n<=0) exit(1);
	if(xmax==xmin){xmin=0.0;xmax=1.0;}
	for(I i=0; i<n; i++) printf("%g\n", xmin+(F)i*(xmax-xmin)/(F)(n-1));
}
